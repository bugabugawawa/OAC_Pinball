.macro appendRankings()
	li a7 1024
	la a0 rankings
	li a1 9
	ecall
	
	li a7 64
	la a1 rankAppend
	li a2 12
	ecall
	
	li a7 8
	li a1 4
	la a0 playersName
	ecall
	
	li a0 3
	
	li a7 64
	la a1 playersName
	li a2 4
	ecall
	
	li a0 3
	
	li a7 64
	la a1 rankAppend
	addi a1 a1 40
	li a2 12
	ecall
	
	li a7 8
	li a1 4
	la a0 playersName
	ecall
	
	li a0 3
	
	li a7 64
	la a1 playersName
	li a2 4
	ecall
	
	li a7 57
	ecall
.end_macro
